<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    <script src="~/Scripts/site.js"></script>
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container body-content">

        @RenderBody()

        <div>
            <a href="@Url.Action("Index","UploadFiles",new { area = "Camera" })">Go to Cammer Upload Page</a> |
            <a href="@Url.Action("iframe","UploadFiles",new { area = "Camera" })">iframe</a> |
            <a href="@Url.Action("hash5","Home",new { area = "" })">是否支持H5</a> |
            <a href="@Url.Action("hascamera","Home",new { area = "" })">是否支持hascamera</a> |
        </div>

        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Sample Code EntityFrameWork</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
